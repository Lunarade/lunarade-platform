                            b 50,000 
                            | requests per month
                        .feature 
                            b Unlimited 
                            | redis explorer connections
                        .feature 
                            b Dedicated 
                            | hosting
                    .btn.btn-primary Choose
                .package
                    .price 290$
                    .timespan per month
                    .title Enterprise
                    .features
